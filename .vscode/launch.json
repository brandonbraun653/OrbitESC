{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "J-Link",
      "cwd": "${workspaceRoot}",
      "executable": "./artifacts/last_build_version/sys_image",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "jlink",
      "interface": "swd",
      "device": "STM32L432KB",
      "svdFile": "${workspaceFolder}/lib/CommonTools/svd/STM32L4x2.svd",
      "gdbPath": "/usr/bin/arm-none-eabi-gdb"
    },
    {
      "name": "Black Magic Probe",
      "cwd": "${workspaceRoot}",
      "executable": "./artifacts/last_build_version/sys_image",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "bmp",
      "interface": "swd",
      "device": "STM32L432KB",
      "svdFile": "${workspaceFolder}/lib/CommonTools/svd/STM32L4x2.svd",
      "gdbPath": "/usr/bin/arm-none-eabi-gdb",
      "BMPGDBSerialPort": "/dev/ttyACM0",
      "preRestartCommands": [
          "enable breakpoint"
      ]
    },
  ]
}