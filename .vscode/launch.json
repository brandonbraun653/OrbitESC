{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "gdb",
      "request": "launch",
      "name": "Simulator",
      "target": "OrbitESC",
      "cwd": "${workspaceRoot}/artifacts/last_build_version",
      "valuesFormatting": "parseText"
    },
    {
      "name": "J-Link OrbitESC V3",
      "cwd": "${workspaceRoot}",
      "executable": "./build/artifacts/last_build_version/OrbitESC",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "jlink",
      "interface": "swd",
      "device": "STM32F446RE",
      "svdFile": "${workspaceFolder}/lib/CommonTools/svd/STM32F446.svd",
      "gdbPath": "arm-none-eabi-gdb",
    },
    {
      "name": "BMP OrbitESC V3",
      "cwd": "${workspaceRoot}",
      "executable": "./build/artifacts/last_build_version/OrbitESC",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "bmp",
      "interface": "swd",
      "device": "STM32F446RE",
      "svdFile": "${workspaceFolder}/lib/CommonTools/svd/STM32F446.svd",
      "gdbPath": "arm-none-eabi-gdb",
      "BMPGDBSerialPort": "/dev/ttyACM0",
      "preRestartCommands": [
          "enable breakpoint"
      ]
    }
  ]
}